int list add_list(int list n, int list m) {
	float list out;
	float list aux;
	int list aux2;

	aux2 = m;

	for (aux = n; aux != NIL; aux = !aux) {
		int element;

		element = ?aux2;
		aux2 = !aux2;

		int e2;
		e2 = ?aux;

		out = e2 + element : out;
	}

	return out;
}

int main() {
